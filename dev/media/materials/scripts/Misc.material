material Misc/Axes
{
	technique
	{
		pass
		{
			lighting off
			scene_blend modulate
			depth_check off
			depth_write off
			cull_hardware none

			texture_unit
			{
				texture axes.png
			}
		}
	}
}

material Misc/BoundingQuadrangle
{
	technique
	{
		pass
		{
			lighting off			
			depth_check off
			depth_write off
			cull_hardware none

			texture_unit
			{
				texture axes.png
			}
		}
	}
}

material PreloaderMaterial
{
    technique
    {
        pass
        {
            ambient 1.0 1.0 1.0
            diffuse 1.0 1.0 1.0
            specular 1.0 1.0 1.0
            emissive 0.0 0.0 0.0
           
            texture_unit
            {
                texture axes.png
            }
        }
    }
}