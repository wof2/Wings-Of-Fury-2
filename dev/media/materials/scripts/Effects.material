material Effects/Submerge : AEffect
{

  technique Diffuse
  {
       pass 0
       {
          texture_unit 0
          { 
              anim_texture Submerge.png 17 0
          }
       }
  }

	technique Fallback
	{
		pass
		{
			ambient 1 1 1
			diffuse 1 1 1	
			lighting off					
			scene_blend alpha_blend		
			depth_write off
			texture_unit
			{
              anim_texture SubmergeAB.png 14 0
			        alpha_op_ex modulate src_texture src_manual 0.5 0.5			
			}		

		}

	}
	
}


