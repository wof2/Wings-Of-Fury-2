// Additive
material AEffect:NoFog
{

	technique Diffuse
	{
      pass 0
      {
      
        lighting on				
        scene_blend add
        depth_write off
        cull_hardware none
        cull_software none
        
        texture_unit 0
        { 
            
        }
     
      }
		
	}
	
